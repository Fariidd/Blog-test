<div class="p-4">
    <h4 class="fst-italic">Categories</h4>
    <ol class="list-unstyled mb-0">
        {% for categorie in categories %}
            <li>
                <a class="text-decoration-none" href="{{ path('app_categorie_show',{ 'id': categorie.id }) }}">{{ categorie.name }}</a>
            </li>
        {% endfor %}
    </ol>
</div>